<%- include('Header') %>


      <!--end::Sidebar-->
      <!--begin::App Main-->
      <main class="app-main">
        <!--begin::App Content Header-->
        <div class="app-content-header">
          <!--begin::Container-->
          <div class="container-fluid">
            <!--begin::Row-->
            <div class="row">
              <div class="col-sm-6"><h3 class="mb-0">Simple Tables</h3></div>
              <div class="col-sm-6">
                <ol class="breadcrumb float-sm-end">
                  <li class="breadcrumb-item"><a href="#">Home</a></li>
                  <li class="breadcrumb-item active" aria-current="page">Simple Tables</li>
                </ol>
              </div>
            </div>
            <!--end::Row-->
          </div>
          <!--end::Container-->
        </div>
        <!--end::App Content Header-->
        <!--begin::App Content-->
        <div class="app-content">
          <!--begin::Container-->
          <div class="container-fluid">
            <!--begin::Row-->
            <div class="row">
                <div class="card mb-4">
                  <div class="card-header"><h3 class="card-title">ViewAdmin Datax    Table</h3></div>
                  <!-- /.card-header -->
                  <div class="card-body">
                    <table class="table table-bordered">
                      <thead>
                        <tr>
                          <th style="width: 10px">No</th>
                          <th>Name</th>
                          <th>Email</th>
                          <th>Gender</th>
                          <th>Hobby</th>
                          <th>City</th>
                          <th>Message</th>
                          <th>Image</th>
                          <th style="width: 40px">Actions</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% GetData.map((v,i)=>{ %>
                            <tr class="align-middle">
                                <td><%= i+1 %></td>
                                <td><%= v.name %></td>
                                <td><%= v.email %></td>
                                <td><%= v.gender %></td>
                                <td><%= v.hobby %></td>
                                <td><%= v.city %></td>
                                <td><%= v.message %></td>
                                <td>
                                    <img src="<%= v.image %>" width="100"/>
                                </td>
                                <td>
                                    <a href="/deleteData/<%= v.id %>"><button class="btn  btn-danger">Delete</button></a>
                                    <br/><br/>
                                    <a href="/updateData/?uId=<%= v.id %>"><button class="btn btn-success">Update</button></a>
                                </td>
                            </tr>
                        <% }) %>
                        
                      </tbody>
                    </table>
                  </div>
                  <!-- /.card-body -->
                  <div class="card-footer clearfix">
                    <ul class="pagination pagination-sm m-0 float-end">
                      <li class="page-item"><a class="page-link" href="#">&laquo;</a></li>
                      <li class="page-item"><a class="page-link" href="#">1</a></li>
                      <li class="page-item"><a class="page-link" href="#">2</a></li>
                      <li class="page-item"><a class="page-link" href="#">3</a></li>
                      <li class="page-item"><a class="page-link" href="#">&raquo;</a></li>
                    </ul>
                  </div>
                </div>
              <!-- /.col -->
            </div>
            <!--end::Row-->
          </div>
          <!--end::Container-->
        </div>
        <!--end::App Content-->
      </main>
      <!--end::App Main-->
      <!--begin::Footer-->
      

<%- include('Footer') %>